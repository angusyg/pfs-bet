{
  "name": "pfs-bet",
  "version": "1.0.0",
  "description": "Foot betting application based on Nean stack",
  "main": "./backend/bin/www",
  "scripts": {
    "test": "cross-env NODE_ENV=test nyc --reporter=html --reporter=text mocha backend/**/*.spec.js test/tests.js --timeout 10000 --exit",
    "test:ti": "cross-env NODE_ENV=test mocha test/tests.js --timeout 10000 --exit",
    "test:tu": "cross-env NODE_ENV=test mocha backend/**/*.spec.js --timeout 10000 --exit",
    "dev": "concurrently \"npm run dev:back\" \"npm run dev:front\"",
    "dev:back": "cross-env NODE_ENV=development cross-env DEBUG=nean:* nodemon",
    "dev:front": "cross-env NODE_ENV=development gulp",
    "start": "node ./backend/bin/www",
    "build": "cross-env NODE_ENV=production gulp",
    "postinstall": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/angusyg/pfs-bet.git"
  },
  "keywords": [
    "nodejs",
    "angularjs",
    "express",
    "nedb"
  ],
  "author": "angusyg",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/angusyg/pfs-bet/issues"
  },
  "nyc": {
    "exclude": [
      "**/*.spec.js",
      "test/**/*.js"
    ],
    "temp-directory": "./coverage/.nyc_output"
  },
  "homepage": "https://github.com/angusyg/pfs-bet#readme",
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.1.0",
    "@uirouter/angularjs": "^1.0.18",
    "angular": "^1.7.2",
    "angular-animate": "^1.7.2",
    "angular-messages": "^1.7.2",
    "angular-storage": "0.0.15",
    "angular-translate": "^2.18.1",
    "angular-translate-loader-partial": "^2.18.1",
    "bcrypt": "^2.0.1",
    "body-parser": "^1.18.3",
    "bootstrap": "^4.1.1",
    "camo": "^0.12.3",
    "compression": "^1.7.2",
    "continuation-local-storage": "^3.2.1",
    "cors": "^2.8.4",
    "cross-env": "^5.2.0",
    "cross-var": "^1.1.0",
    "csurf": "^1.9.0",
    "express": "^4.16.3",
    "express-pino-logger": "^3.0.1",
    "helmet": "^3.12.0",
    "jquery": "^3.3.1",
    "jsonwebtoken": "^8.3.0",
    "kind-of": "^6.0.2",
    "open-iconic": "^1.1.1",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "pino": "^4.17.3",
    "pino-multi-stream": "^3.1.2",
    "ui-bootstrap4": "^3.0.3",
    "uuid": "^3.2.1"
  },
  "devDependencies": {
    "angular-jsdoc": "git+https://github.com/Monsido/angular-jsdoc.git",
    "chai": "^4.1.2",
    "chai-as-promised": "^7.1.1",
    "chai-http": "^4.0.0",
    "concurrently": "^3.5.1",
    "del": "^3.0.0",
    "eslint": "^4.19.1",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-angular": "^3.3.0",
    "eslint-plugin-import": "^2.12.0",
    "gulp": "^3.9.1",
    "gulp-clean-css": "^3.9.4",
    "gulp-concat": "^2.6.1",
    "gulp-connect": "^5.5.0",
    "gulp-eslint": "^4.0.2",
    "gulp-html-replace": "^1.6.2",
    "gulp-if": "^2.0.2",
    "gulp-jsdoc3": "^2.0.0",
    "gulp-livereload": "^3.8.1",
    "gulp-load-plugins": "^1.5.0",
    "gulp-rename": "^1.3.0",
    "gulp-sass": "^4.0.1",
    "gulp-shell": "^0.6.5",
    "gulp-uglify": "^3.0.0",
    "gulp-uglify-es": "^1.0.4",
    "gulp-util": "^3.0.8",
    "istanbul": "^0.4.5",
    "jsdoc-http-plugin": "^0.3.0",
    "mocha": "^5.2.0",
    "nodemon": "^1.17.5",
    "nyc": "^12.0.2",
    "proxyquire": "^2.0.1",
    "pump": "^3.0.0",
    "run-sequence": "^2.2.1",
    "sinon": "^6.0.0",
    "supertest": "^3.1.0"
  }
}
